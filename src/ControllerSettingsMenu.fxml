<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerSettingsMenu">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0">
         <image>
            <Image url="@pics/settings_menu_pics/settings_menu_back_ground.jpg" />
         </image>
      </ImageView>
      <JFXComboBox fx:id="timeComboBox" layoutX="188.0" layoutY="46.0" onMouseClicked="#chooseTime" prefHeight="31.0" prefWidth="152.0" promptText="Choose Duration" />
      <ImageView layoutX="116.0" layoutY="30.0">
         <image>
            <Image url="@pics/settings_menu_pics/time_setting_pic.png" />
         </image>
      </ImageView>
      <ImageView fx:id="backBtn" fitHeight="96.0" fitWidth="99.0" layoutY="-2.0" onMouseClicked="#goBack" onMouseEntered="#makeBackBtnOpaque" onMouseExited="#makeBackBtnTransparent" preserveRatio="true" style="-fx-opacity: 0.6;">
         <image>
            <Image url="@pics/back_button.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
