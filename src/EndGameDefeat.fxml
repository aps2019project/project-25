<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="813.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EndGameDefeat">
   <children>
      <ImageView fitHeight="813.0" fitWidth="1500.0">
         <image>
            <Image url="@pics/defeat/defeat_background.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="backBtn" onMouseClicked="#goBack" onMouseEntered="#makeBackBtnOpaque" onMouseExited="#makeBackBtnTransparent" style="-fx-opacity: 0.6;">
         <image>
            <Image url="@pics/back_button.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
